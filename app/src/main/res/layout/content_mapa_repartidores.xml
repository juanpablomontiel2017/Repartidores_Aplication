<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:mapbox="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"


    android:layout_width="match_parent"
    android:layout_height="match_parent"

    mapbox:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".Mapa_Repartidores"
    tools:showIn="@layout/activity_mapa_repartidores"
    android:orientation="vertical">





    <com.mapbox.mapboxsdk.maps.MapView
        android:id="@+id/mapViewRepartidores"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        mapbox:mapbox_cameraTargetLat="-26.784317"
        mapbox:mapbox_cameraTargetLng="-60.446838"
        mapbox:mapbox_cameraZoom="15"/>






</LinearLayout>